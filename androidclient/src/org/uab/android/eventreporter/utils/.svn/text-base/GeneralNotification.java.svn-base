package org.uab.android.eventreporter.utils;

public class GeneralNotification {

	private int id;
	private String description;
	private String lineReference;
	private int severity;
	private int ttl;
	private long lastUpdateTime;
	private String username;
	private double latitude;
	private double longitude;
	private int status;
	
	public GeneralNotification() {
		
	}
	
	public GeneralNotification(int id, String description, int severity, int ttl, 
			long lastUpdateTime, String username, double latitude, double longitude, 
			int status) {
		this.id = id;
		this.description = description;
		this.severity = severity;
		this.ttl = ttl;
		this.lastUpdateTime = lastUpdateTime;
		this.username = username;
		this.latitude = latitude;
		this.longitude = longitude;
		this.status = status;
	}
	
	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		this.id = id;
	}
	
	public String getDescription() {
		return description;
	}
	
	public void setDescription(String description) {
		this.description = description;
	}
	
	public String getLineReference() {
		return lineReference;
	}
	
	public void setLineReference(String lineRef) {
		this.lineReference = lineRef;
	}
	
	public int getSeverity() {
		return severity;
	}
	
	public void setSeverity(int severity) {
		this.severity = severity;
	}
	
	public int getTTL() {
		return ttl;
	}
	
	public void setTTL(int ttl) {
		this.ttl = ttl;
	}
	
	public long getLastUpdateTime() {
		return lastUpdateTime;
	}

	public void setLastUpdateTime(long lastUpdateTime) {
		this.lastUpdateTime = lastUpdateTime;
	}
	
	public String getUsername() {
		return username;
	}
	
	public void setUsername(String username) {
		this.username = username;
	}
	
	public double getLatitude() {
		return latitude;
	}
	
	public void setLatitude(double latitude) {
		this.latitude = latitude;
	}
	
	public double getLongitude() {
		return longitude;
	}
	
	public void setLongitude(double longitude) {
		this.longitude = longitude;
	}
	
	public int getStatus() {
		return status;
	}
	
	public void setStatus(int status) {
		this.status = status;
	}
}
